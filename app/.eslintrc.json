{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "WeDeploy": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "error",
        "constructor-super": "warn",
        "no-const-assign": "warn",
        "no-redeclare": "error",
        "no-this-before-super": "warn",
        "no-undef": "error",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "no-use-before-define": [2, { "functions": false }],
        "semi": "error",
        "valid-typeof": "warn"
    }
}